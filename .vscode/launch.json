{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch fitlog",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dist/fitlog",
            "args": ["show", "exercise", "--from", "2025-09-11"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "Path",
                    "value": "C:/msys64/mingw64/ucrt64/bin;${env:Path}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/MinGW/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build fitlog"
        }
    ]
}

